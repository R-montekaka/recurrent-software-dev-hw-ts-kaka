  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
